{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://irukandjilabs.com/apify/spec/endpoint/root.json",
  "type": "object",
  "title": "The Endpoint Schema",
  "required": [
    "request",
    "response"
  ],
  "properties": {
    "request": {
      "$id": "#/properties/request",
      "type": "object",
      "title": "The Request Schema",
      "required": [
        "fields"
      ],
      "properties": {
        "fields": {
          "$id": "#/properties/fields",
          "type": "object",
          "title": "The Fields Schema",
          "patternProperties": {
            "^.*$": {
              "oneOf": [
                { "$ref": "/apify/spec/schemas/types/string.json" },
                { "$ref": "/apify/spec/schemas/types/integer.json" },
                { "$ref": "/apify/spec/schemas/types/float.json" },
                { "$ref": "/apify/spec/schemas/types/array.json" },
                { "$ref": "/apify/spec/schemas/types/date.json" },
                { "$ref": "/apify/spec/schemas/types/boolean.json" },
                { "$ref": "/apify/spec/schemas/types/reference.json" },
                { "$ref": "/apify/spec/schemas/types/association.json" }
              ]
            }
          },
          "additionalProperties": false
        }
      }
    },
    "response": {
      "$id": "#/properties/response",
      "type": "object",
      "title": "The Response Schema",
      "required": [
        "fields"
      ],
      "properties": {
        "fields": {
          "$id": "#/properties/fields",
          "type": "object",
          "title": "The Fields Schema",
          "patternProperties": {
            "^.*$": {
              "oneOf": [
                { "$ref": "/apify/spec/schemas/types/string.json" },
                { "$ref": "/apify/spec/schemas/types/integer.json" },
                { "$ref": "/apify/spec/schemas/types/float.json" },
                { "$ref": "/apify/spec/schemas/types/array.json" },
                { "$ref": "/apify/spec/schemas/types/date.json" },
                { "$ref": "/apify/spec/schemas/types/boolean.json" },
                { "$ref": "/apify/spec/schemas/types/reference.json" },
                { "$ref": "/apify/spec/schemas/types/association.json" }
              ]
            }
          },
          "additionalProperties": false
        }
      }
    }
  },
  "additionalProperties": false
}